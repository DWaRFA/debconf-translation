#: ../Debconf/AutoSelect.pm:76
#, perl-format
msgid "falling back to frontend: %s"
msgstr "thì rút lui lối vào: %s"

#: ../Debconf/AutoSelect.pm:84
#, perl-format
msgid "unable to initialize frontend: %s"
msgstr "không thể khởi động tiền tiêu: %s"

#: ../Debconf/AutoSelect.pm:90
#, perl-format
msgid "Unable to start a frontend: %s"
msgstr "Không thể bắt đầu tiền tiêu: %s"

#: ../Debconf/Config.pm:128
msgid "Config database not specified in config file."
msgstr "Chưa ghi rõ cơ sở dữ liệu cấu hình trong tập tin cấu hình."

#: ../Debconf/Config.pm:132
msgid "Template database not specified in config file."
msgstr "Chưa ghi rõ cơ sở dữ liệu mẫu trong tập tin cấu hình."

#: ../Debconf/Config.pm:137
msgid ""
"The Sigils and Smileys options in the config file are no longer used. Please "
"remove them."
msgstr "Không còn dùng lại những tùy chọn Sigils và Smileys. Hãy loại bỏ suốt."

#: ../Debconf/Config.pm:148
#, perl-format
msgid "Problem setting up the database defined by stanza %s of %s."
msgstr ""
"Gặp lỗi khi thiết lập cơ sở dữ liệu đã định nghĩa do đoạn dòng %s của %s."

#: ../Debconf/Config.pm:197
msgid ""
"  -f,  --frontend\t\tSpecify debconf frontend to use.\n"
"  -p,  --priority\t\tSpecify minimum priority question to show.\n"
"       --terse\t\t\tEnable terse mode.\n"
msgstr ""
"  -f,  --frontend\t\tGhi rõ _tiền tiêu_ debconf cần sử dụng.\n"
"  -p,  --priority\t\tSGhi rõ _ưu tiên_ tối thiểu cân hiển thị.\n"
"       --terse\t\t\tHiệu lực chế độ _ngắn gọn_.\n"

#: ../Debconf/Config.pm:277
#, perl-format
msgid "Ignoring invalid priority \"%s\""
msgstr "Đang bỏ qua ưu tiên không hợp lê \"%s\"."

#: ../Debconf/Config.pm:278
#, perl-format
msgid "Valid priorities are: %s"
msgstr "Ưu tiên hợp lệ: %s"

#: ../Debconf/Element/Editor/Boolean.pm:30
#: ../Debconf/Element/Editor/Multiselect.pm:31
#: ../Debconf/Element/Editor/Select.pm:31
msgid "Choices"
