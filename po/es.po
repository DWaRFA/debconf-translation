msgstr ""
"Nota: Debconf está ejecutándose en modo web. Vaya a http://localhost:%i/"

#: ../Debconf/FrontEnd/Web.pm:165
msgid "Back"
msgstr "Anterior"

#: ../Debconf/FrontEnd/Web.pm:167
msgid "Next"
msgstr "Siguiente"

